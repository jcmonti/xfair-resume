extends base
block extra-headers

  script(src='/static/datatables/media/js/jquery.dataTables.min.js')
  script(src='/static/datatables/media/js/dataTables.bootstrap.js')

  link(rel='stylesheet', href='/static/datatables/media/css/dataTables.bootstrap.min.css')

  link(rel='stylesheet', href='/static/css/employers.css')
  script(src='/static/js/employers.js')

block content
  div.row
    div.col-sm-10.col-sm-offset-1
      h1 Resume Search
      table#search.display.table.table-striped.table-bordered.table-hover
